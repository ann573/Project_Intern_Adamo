    "check:lint": "eslint . --fix --quiet",
    "check:prettier": "prettier --write --ignore-unknown \"**/*\" ",
    "check:all": "npm run check:prettier && npm run check:lint && npm run check:type",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
