{
  "name": "project_intern",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "server": "json-server --delay 1200 --watch db.json --port 3000",
    "lint": "eslint .",
    "format": "prettier . --write",
    "preview": "vite preview",
