    "react-spinners": "^0.15.0",
    "react-to-print": "^3.0.6",
    "slick-carousel": "^1.8.1",
    "sonner": "^2.0.3",
    "styled-components": "^6.1.17",
    "tailwind-merge": "^3.2.0",
